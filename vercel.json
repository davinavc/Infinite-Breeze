{
    "version": 2,
    "framework": null,
    "functions": {
        "api/index.php": { "runtime": "vercel-php@0.6.0" }
    },
    "routes": [
        { "src": "/build/(.*)", "dest": "/public/build/$1" },
        { "src": "/css/(.*)", "dest": "/public/css/$1" },
        { "src": "/img/(.*)", "dest": "/public/img/$1" },
        { "src": "/storage/(.*)", "dest": "/public/storage/$1" },
        { "src": "/js/(.*)", "dest": "/public/js/$1" },
        { "src": "/(.*)", "dest": "/api/index.php" }
    ],
    "buildCommand": "vite build",
    "outputDirectory": "public",
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "APP_URL": "https://davina-portofolio-gamma.vercel.app",
        "APP_KEY": "base64:MjGN08G4plku/F3+5YeboxY8Ti7nWmKYajuEDq6lN4E=",
        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_EVENTS_CACHE": "/tmp/events.php",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_ROUTES_CACHE": "/tmp/routes.php",
        "APP_SERVICES_CACHE": "/tmp/services.php",
        "VIEW_COMPILED_PATH": "/tmp",
        "CACHE_DRIVER": "array",
        "LOG_CHANNEL": "stderr",
        "SESSION_DRIVER": "cookie",
        "DB_CONNECTION": "mysql",
        "DB_HOST": "2tkpa7.h.filess.io",
        "DB_PORT": "3306",
        "DB_DATABASE": "Portofolio_lifeteeth",
        "DB_USERNAME": "Portofolio_lifeteeth"
    }
}